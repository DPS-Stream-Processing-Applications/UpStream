# NOTE: It should be possible to bring your individual ssh config with you.
include ~/.ssh/config

Host jump
    HostName 129.114.109.220
    User cc
    # IdentityFile ./jump_key

Host k3s-server
    HostName 10.52.0.117
    User cc
    ProxyJump jump
    # IdentityFile ./cluster_key

Host k3s-agent-1
    HostName 10.52.2.216
    User cc
    ProxyJump jump
    # IdentityFile ./cluster_key

Host k3s-agent-2
    HostName 10.52.3.16
    User cc
    ProxyJump jump
    # IdentityFile ./cluster_key
